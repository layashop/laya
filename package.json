{
  "name": "laya_shop",
  "version": "0.1.0",
  "dependencies": {
    "emoji-mart": "^3.0.0",
    "lodash": "^4.17.20",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-tiny-link": "^3.6.0",
    "theme-ui": "^0.3.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.14.0",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.2",
    "gulp-uglify-es": "^1.0.4",
    "npx": "^10.2.2",
    "parcel-bundler": "^1.12.4",
    "pixrem": "^5.0.0",
    "react-select": "^3.1.0",
    "tailwindcss": "^1.8.12"
  },
  "engines": {
    "node": ">=8"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "dev": "npm run jsx-watch",
    "build": "npm run jsx-build",
    "jsx-watch": "parcel watch ./laya_shop/static/jsx/*.jsx --out-dir ./laya_shop/static/dist/js/",
    "jsx-build": "parcel build ./laya_shop/static/jsx/*.jsx --out-dir ./laya_shop/static/dist/js/",
    "generate-tailwind": "tailwindcss build > ./laya_shop/static/dist/css/tailwind.css",
    "base-dev": "yarn run generate-tailwind && yarn run jsx-watch"
  }
}
